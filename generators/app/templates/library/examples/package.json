{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    <% if (typescript) { -%>
    "validate": "svelte-check",
    <% } -%>
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "rollup": "^2.21.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    <% if (has_preprocessors) { -%>
    "svelte-preprocess": "^4.0.8",
    <% if (sass) { -%>
    "sass": "^1.26.10",
    "postcss": "^7.0.32",
    <% } -%>
    <% if (typescript) { -%>
    "typescript": "^3.9.7",
    "@rollup/plugin-typescript": "^5.0.2",
    "svelte-check": "^0.1.55",
    "tslib": "^2.0.0",
    "@tsconfig/svelte": "^1.0.2",
    <% } -%>
    <% } -%>
    "svelte": "^3.24.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.3",
    "<%= package_name %>": "^0.0.1"
  }
}
