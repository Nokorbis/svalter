{
    "name": "<%= package_name %>",
    "version": "1.0.0",
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv public"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^13.0.0",
        "@rollup/plugin-node-resolve": "^8.1.0",
        "rollup": "^2.21.0",
        "rollup-plugin-livereload": "^1.3.0",
        "rollup-plugin-svelte": "^5.2.3",
        "rollup-plugin-terser": "^6.1.0",
        <% if (support_preprocessors.length > 1) { %>
        "svelte-preprocess": "^4.0.6",
        <% if (support_preprocessors.includes('sass')) { %>
        "node-sass": "^4.14.1",
        <% } %>
        <% if (support_preprocessors.includes('typescript')) { %>
        "typescript": "^3.9.6",
        "@rollup/plugin-typescript": "^5.0.1",
        <% } %>
        <% } %>
        "svelte": "^3.24.0"
    },
    "dependencies": {
        "sirv-cli": "^1.0.3"
    }
}
